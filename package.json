{
  "name": "akualis-landing-page",
  "version": "0.0.5",
  "description": "Akualis landing page",
  "productName": "Akualis",
  "author": "m-xime <m@xime.dev>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m ssr",
    "build": "quasar build -m ssr",
    "postinstall": "quasar prepare",
    "staging": "NODE_ENV=development && npx quasar build -m ssr && node dist/ssr/index.js",
    "start": "node dist/ssr/index.js",
    "gcloud:login": "gcloud auth login && gcloud config set project akualis-landing-page",
    "deploy:production": "npm run build && gcloud app deploy app.yaml --project=akualis-landing-page"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.17",
    "express": "^5.1.0",
    "quasar": "^2.18.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@intlify/unplugin-vue-i18n": "^6.0.5",
    "@quasar/app-vite": "^2.2.0",
    "@types/node": "^22.14.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-sitemap": "^0.7.1",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
