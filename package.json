{
  "name": "akualis-landing-page",
  "version": "0.0.1",
  "description": "Akualis landing page",
  "productName": "Akualis",
  "author": "m-xime <m@xime.dev>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "npx quasar dev -m ssr",
    "build": "npx quasar build -m ssr",
    "staging": "NODE_ENV=development && npx quasar build -m ssr && node dist/ssr/index.js",
    "start": "node dist/ssr/index.js",
    "gcloud:login": "gcloud auth login && gcloud config set project akualis-landing-page",
    "deploy:production": "npm run build && gcloud app deploy app.yaml --project=akualis-landing-page"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "quasar": "^2.6.0",
    "sitemap": "^7.1.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue-server-renderer": "^2.7.14",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
